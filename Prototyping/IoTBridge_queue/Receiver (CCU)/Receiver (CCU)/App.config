<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
  <appSettings>
    <!-- Service Bus specific app setings for messaging connections -->
    <add key="ConnectionString"
         value="[your conection string]"/>
    <add key="QueueName_1"
         value="[name of the queue for the receiver receiving the command from the user]"/>
    <add key="QueueName_2"
         value="[name of the queue for the receiver sending the value to the user]"/>
  </appSettings>
</configuration>