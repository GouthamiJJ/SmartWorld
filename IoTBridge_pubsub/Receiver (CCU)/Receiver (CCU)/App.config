<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
  <appSettings>
    <!-- Service Bus specific app setings for messaging connections -->
    <add key="ConnectionString"
         value="[name of the connection string]"/>
    <add key="QueueName_1"
         value="[name of queue]"/>
    <add key="TopicName_1"
         value="[name of topic]"/>
    <add key="SubscriptionName_1"
         value="[name of subscription]"/>
  </appSettings>
</configuration>